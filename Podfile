     pod 'CryptoSwift'
     pod 'SwiftyRSA'
     pod 'Alamofire', '5.0.0-rc.3'
     pod 'SwiftyJSON'
     pod 'FoxOnePaySDK', :path => './'
 end

target 'SDKOCDemo' do
    pod 'CryptoSwift'
